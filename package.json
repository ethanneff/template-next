{
  "name": "@ethanneff/template-next",
  "version": "1.2.8",
  "description": "A sample web template using NextJS and Typescript",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@ethanneff"
  },
  "repository": "https://github.com/ethanneff/template-next",
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 3333",
    "lint": "next lint && tsc && eslint '**/*.[jt]s?(x)' --cache --fix --max-warnings=0",
    "test": "jest -u --coverage --cache --config ./.github/jest.config.js",
    "watch": "jest -u --watchAll --cache --config ./.github/jest.config.js",
    "prepare": "husky install",
    "ci-prettify": "prettier --write .",
    "ci-dupe": "jsinspect ./src",
    "ci-install": "yarn install --frozen-lockfile",
    "ci-build": "touch .nojekyll && yarn build && yarn export",
    "ci-lint": "tsc && eslint '**/*.[jt]s?(x)' --cache --max-warnings=0",
    "ci-test": "TZ=America/Los_Angeles jest --ci --forceExit --detectOpenHandles --silent --coverage --coverageReporters=text-summary --config ./.github/jest.config.js",
    "ci-release": "semantic-release -e ./.github/release.config.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.6.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "swr": "0.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.14",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "jsinspect": "0.12.7",
    "normalize.css": "8.0.1",
    "prettier": "2.3.2",
    "semantic-release": "17.4.4",
    "typescript": "4.3.5"
  }
}
